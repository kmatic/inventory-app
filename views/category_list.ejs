<!DOCTYPE html>
<html>
  <head>
    <%- include('./partials/head', {title: title}); %>
  </head>
  <body>
    <header>
      <%- include('./partials/header'); %>
    </header>
    <main>
        <h1><%= title %></h1>
        <ul>
        <% list_categories.forEach((category) => { %>
            <li>
                <a href="<%= category.url %>"><%= category.name %></a> 
            </li>
        <% }); %>
        </ul>
        <div style='margin-top:30px'>
            <p><a href='/catalog/category/create'>New category</a></p>
        </div>
    </main>
  </body>
</html>